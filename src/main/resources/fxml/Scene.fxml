<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>
<AnchorPane id="AnchorPane" prefHeight="400" prefWidth="300" xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/8.0.40" fx:controller="com.kodcu.FXMLController">
    <children>
        <VBox>
            <Button fx:id="button" onAction="#handleButtonAction" text="Let's Crash"/>
            <WebView fx:id="webView" VBox.vgrow="ALWAYS"/>
        </VBox>
    </children>
</AnchorPane>
